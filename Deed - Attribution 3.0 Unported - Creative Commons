        <use href="/wp-content/themes/vocabulary-theme/vocabulary/svg/cc/icons/cc-icons.svg#cc-logo">
        </use>
       </svg>
      </span>
      <span class="cc-icon">
       <svg viewBox="0 0 30 30">
